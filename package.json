{
  "name": "fhs-react-start-kit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/addon-knobs": "^6.1.20",
    "formik": "^2.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start:server": "json-server --watch db.json --port 3001",
    "test": "react-scripts test",
    "start": "concurrently -n app,storybook 'yarn start:app' 'yarn start:storybook'",
    "start:app": "react-scripts start",
    "start:storybook": "start-storybook -p 9009 -s public",
    "build": "concurrently -n app,storybook 'yarn build:app' 'yarn build:storybook'",
    "build:app": "react-scripts build",
    "build:storybook": "build-storybook -s storybook",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js  --fix .",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "semi": 0,
      "no-console": 0
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/addons": "^6.1.20",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.20",
    "@testing-library/react": "^11.2.5",
    "concurrently": "^6.0.0",
    "css-loader": "^5.1.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.2",
    "husky": "^5.1.3",
    "json-server": "^0.16.3",
    "node-sass": "^5.0.0",
    "node-sass-magic-importer": "^5.3.2",
    "proxyquire": "^2.1.3",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.8",
    "sass-loader": "^10.0.0",
    "standard": "^16.0.3",
    "style-loader": "^2.0.0"
  }
}
